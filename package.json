{
  "name": "test01",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.21.1",
    "vue": "^3.2.31",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.6.0",
    "prettier": "2.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": "off",
      "no-unreachable": "off",
      "no-debugger": "off",
      "no-constant-condition": [
        2,
        {
          "checkLoops": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}